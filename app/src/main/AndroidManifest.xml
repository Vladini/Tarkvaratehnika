<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.traveleasy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.user.traveleasy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.user.traveleasy.ActiveTripsActivity" />
        <activity android:name="com.example.user.traveleasy.BudgetActivity" />
        <activity android:name="com.example.user.traveleasy.EventActivity" />
        <activity android:name="com.example.user.traveleasy.RecentTripsActivity" />
        <activity android:name="com.example.user.traveleasy.TripProfileActivity" />
        <activity android:name="com.example.user.traveleasy.TripsCalendarActivity" />
        <activity android:name="com.example.user.traveleasy.TripsNotesActivity" />
        <activity android:name="com.example.user.traveleasy.NewTripActivity" />
        <activity android:name="com.example.user.traveleasy.EventProfileActivity"></activity>
    </application>

</manifest>